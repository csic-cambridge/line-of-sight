# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = false
trim_trailing_whitespace = true

[*.md]
max_line_length = off

[*.java]
max_line_length = 120
insert_final_newline = true

#Files to ignore
[nbactions.xml]
indent_style = none
indent_size = none

[*.ts]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
